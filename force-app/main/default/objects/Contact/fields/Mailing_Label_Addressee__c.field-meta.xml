<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <fullName>Mailing_Label_Addressee__c</fullName>
    <description>First line of mailing label: Addresse if specified, otherwise First + Last</description>
    <externalId>false</externalId>
    <formula>IF(Addressee__c = &quot;&quot;,
  FirstName &amp; &quot; &quot; &amp;  LastName,
  Addressee__c
)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Mailing Label: Addressee</label>
    <required>false</required>
    <trackHistory>false</trackHistory>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
